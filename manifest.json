{
  "manifest_version": 3,
  "name": "Link Saver",
  "version": "1.0",
  "description": "Salva e sincronizza i tuoi link preferiti con parole chiave",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}